<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>

<Reseau xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="reseaux.xsd">

	<intervalles>
		<Intervalle id="immo-arret-foch-maison-bleue">
			<pref>00:01:00</pref>
		</Intervalle>
		<Intervalle id="immo-depot-1">
			<min>00:30:00</min>
			<pref>10:00:00</pref>
		</Intervalle>
		<Intervalle id="immo-term-A-1">
			<min>00:10:00</min>
			<pref>00:15:00</pref>
			<max>00:45:00</max>
		</Intervalle>
	</intervalles>

	<arrets>
		<Arret id="foch-haras" nom="Foch Haras" estCommercial="true"
			estEntreeSortieDepot="false" tempsImmobilisation="immo-arret-a-foch-maison-bleue"
			estLieuEchangeConducteur="true" />
		<Arret id="foch-maison-bleue" nom="Foch Maison Bleue"
			estCommercial="true" estEntreeSortieDepot="false"
			tempsImmobilisation="immo-arret-a-foch-maison-bleue"
			estLieuEchangeConducteur="true" />
		<Arret id="angers-roseraie" nom="Angers Roseraie"
			estCommercial="true" estEntreeSortieDepot="false"
			tempsImmobilisation="immo-term-A-1" estLieuEchangeConducteur="true" />
		<Arret id="avrille-ardenne" nom="Avrillé Ardenne"
			estCommercial="true" estEntreeSortieDepot="false"
			tempsImmobilisation="immo-term-A-1" estLieuEchangeConducteur="true" />
		<Arret id="ctt" nom="CTT"
			estCommercial="false" estEntreeSortieDepot="false"
			tempsImmobilisation="immo-depot-1" estLieuEchangeConducteur="true" />
	</arrets>
	
	<depots>
		<Depot ref="ctt" />
	</depots>
	
	<lignes>
		<Ligne id="A">

			<voies>
				<Voie direction="Avrillé">
					<arrets>
						<Arret ref="foch-haras" />
						<Arret ref="foch-maison-bleue" />
					</arrets>
					<terminusDepart>
						<Terminus ref="angers-roseraie" />
					</terminusDepart>
					<terminusArrivee>
						<Terminus ref="avrille-ardenne" />
					</terminusArrivee>
				</Voie>
			</voies>

		</Ligne>
	</lignes>

	<transitions>
		<Transition id="trans-angers-roseraie-foch-haras" duree="00:20:00">
			<arretPrecedent>
				<Arret ref="angers-roseraie" />
			</arretPrecedent>
			<arretSuivant>
				<Arret ref="foch-haras" />
			</arretSuivant>
		</Transition>
		<Transition id="trans-foch-haras-foch-maison-bleue" duree="00:01:00">
			<arretPrecedent>
				<Arret ref="foch-haras" />
			</arretPrecedent>
			<arretSuivant>
				<Arret ref="foch-maison-bleue" />
			</arretSuivant>
		</Transition>
		<Transition id="trans-foch-maison-bleue-avrille-ardenne"
			duree="00:25:00">
			<arretPrecedent>
				<Arret ref="foch-maison-bleue" />
			</arretPrecedent>
			<arretSuivant>
				<Arret ref="avrille-ardenne" />
			</arretSuivant>
		</Transition>
	</transitions>

</Reseau>